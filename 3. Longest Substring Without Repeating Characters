/**
 * @param {string} s
 * @return {number}
 */
var lengthOfLongestSubstring = function(s) {
    let i = 0, chars = '', count = 0, longestSubStr = 0;
    while(i < s.length){
        if(!chars.includes(s[i])){
            count += 1;
            chars += s.slice(i, 1);
        }
        else{
            if(count > longestSubStr) longestSubStr = count;
            chars = s.slice(i, 1);
            count = 1;
        }
    }
    if(count > longestSubStr) longestSubStr = count;
    return longestSubStr;
};
